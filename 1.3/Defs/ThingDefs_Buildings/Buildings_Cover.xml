  <!--==================== SIMPLE CARTETS ======================== -->
  <ThingDef ParentName="BuildingBase">
    <defName>SimpleCarpetDan_q</defName>
    <label>Teppich No. 1</label>
    <description>Einfacher Teppich ohne komplizierte Muster mit eingebautem Elektrodraht.\n Texture: 1.\n Basisästhetik: 3</description>
    <thingClass>Building</thingClass>
    <category>Building</category>
    <graphicData>
      <texPath>Carpets/CapretDan_1</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <linkType>Transmitter</linkType>
      <linkFlags>
        <li>Custom7</li>
      </linkFlags>
    </graphicData>
    <uiIconPath>CarpetIcons/CapretDan_1</uiIconPath>
    <altitudeLayer>Floor</altitudeLayer>
    <pathCost>0</pathCost>
    <pathCostIgnoreRepeat>false</pathCostIgnoreRepeat>
    <passability>Standable</passability>
    <castEdgeShadows>false</castEdgeShadows>
    <fillPercent>0</fillPercent>
    <canOverlapZones>true</canOverlapZones>
    <statBases>
      <MaxHitPoints>60</MaxHitPoints>
      <Beauty>3</Beauty>
      <WorkToBuild>200</WorkToBuild>
      <Flammability>1</Flammability>
    </statBases>
    <building>
      <isEdifice>false</isEdifice>
      <ai_chillDestination>false</ai_chillDestination>
      <isInert>true</isInert>
      <blueprintGraphicData>
        <texPath>Carpets/CarpetQ_Blueprint_Atlas</texPath>
      </blueprintGraphicData>
    </building>
    <placingDraggableDimensions>2</placingDraggableDimensions>
    <stuffCategories>
      <li>Fabric</li>
      <li>Leathery</li>
    </stuffCategories>
    <colorPerStuff>
      <li>
        <stuff>Cloth</stuff>
        <color>(162,157,152)</color>
      </li>
    </colorPerStuff>
    <placeWorkers>
      <li>PlaceWorker_Conduit</li>
    </placeWorkers>
    <comps>
      <li Class="CompProperties_Power">
        <compClass>CompPowerTransmitter</compClass>
        <transmitsPower>true</transmitsPower>
      </li>
    </comps>
    <costStuffCount>10</costStuffCount>
    <costList>
      <Steel>1</Steel>
    </costList>
    <!--filthLeaving>SandbagRubble</filthLeaving-->
    <rotatable>false</rotatable>
    <selectable>false</selectable>
    <neverMultiSelect>false</neverMultiSelect>
    <clearBuildingArea>false</clearBuildingArea>
    <terrainAffordanceNeeded>Plitka</terrainAffordanceNeeded>
    <useStuffTerrainAffordance>true</useStuffTerrainAffordance>
    <designationCategory>FurnitureCarpetsPP</designationCategory>
    <!--staticSunShadowHeight>0.20</staticSunShadowHeight-->
    <repairEffect>ConstructDirt</repairEffect>
    <fertility>0</fertility>
    <researchPrerequisites>
      <li>CarpetsP</li>
    </researchPrerequisites>
  </ThingDef>
